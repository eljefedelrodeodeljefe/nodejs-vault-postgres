{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --no-lazy --inspect"
  },
  "watch": [
    "index.js",
    "lib/",
    "test/",
    "config.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json md"
}
